{
  "name": "number-switcher-3000",
  "version": "1.0.0",
  "description": "An app for dealing with dial-in systems",
  "engine": {
    "npm": "^2.13.2"
  },
  "scripts": {
    "build": "webpack -p && npm run build:assets",
    "build:assets": "cp app/robots.txt public/robots.txt && cp -R app/favicons/* public",
    "build:dev": "webpack -d && npm run build:assets",
    "start": "node api/index.js",
    "test": "eslint . && npm run build:dev && mocha --compilers js:babel/register test/**/*.js",
    "watch": "pm2-dev run api/index.js & (npm run build:assets && webpack -d --watch)"
  },
  "author": "Jeffrey Charles <jeffreycharles@beyondtechnicallycorrect.com>",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffcharles/number-switcher-3000"
  },
  "devDependencies": {
    "babel-eslint": "^3.1.30",
    "bootstrap": "^3.3.4",
    "css-loader": "^0.12.1",
    "eslint": "^0.21.0",
    "eslint-plugin-react": "^2.7.1",
    "file-loader": "^0.8.1",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "mocha": "^2.2.5",
    "node-libs-browser": "^0.5.0",
    "pm2": "^0.14.5",
    "style-loader": "^0.12.2",
    "supertest": "^1.0.1",
    "supertest-as-promised": "^2.0.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.10.5"
  },
  "dependencies": {
    "aws-sdk": "^2.1.42",
    "babel": "^5.8.19",
    "babel-core": "^5.8.19",
    "babel-loader": "^5.3.2",
    "body-parser": "^1.13.1",
    "cookie-parser": "^1.3.4",
    "express": "^4.13.1",
    "express-handlebars": "^2.0.1",
    "flummox": "^3.5.2",
    "immutable": "^3.7.4",
    "rc": "^1.1.0",
    "react": "^0.13.3",
    "react-bootstrap": "^0.24.3",
    "superagent": "^1.2.0",
    "xml2js": "^0.4.9"
  }
}
