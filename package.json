{
  "name": "number-switcher-3000",
  "version": "1.0.0",
  "description": "An app for dealing with dial-in systems",
  "engine": {
    "node": "0.12.3"
  },
  "scripts": {
    "build": "webpack -p && npm run build:assets",
    "build:assets": "cp app/robots.txt public/robots.txt && cp -R app/favicons/* public",
    "build:dev": "webpack -d && npm run build:assets",
    "start": "forever --minUptime 1000 api/run-server.js",
    "test": "eslint . && npm run build && mocha test/**/*.js",
    "watch": "forever --minUptime 1000 -w --watchDirectory api api/run-server.js & npm run build:assets && webpack -d --watch"
  },
  "author": "Jeffrey Charles <jeffreycharles@beyondtechnicallycorrect.com>",
  "license": "BSD",
  "repository": {
    "type": "git",
    "url": "https://github.com/jeffcharles/number-switcher-3000"
  },
  "devDependencies": {
    "babel-core": "^5.4.2",
    "babel-eslint": "^3.1.1",
    "babel-loader": "^5.0.0",
    "bootstrap": "^3.3.4",
    "css-loader": "^0.12.1",
    "eslint": "^0.21.0",
    "file-loader": "^0.8.1",
    "html-webpack-plugin": "^1.3.0",
    "less": "^2.5.0",
    "less-loader": "^2.2.0",
    "node-libs-browser": "^0.5.0",
    "react": "^0.13.3",
    "style-loader": "^0.12.2",
    "url-loader": "^0.5.5",
    "webpack": "^1.9.5",
    "supertest": "~1.0.1",
    "supertest-as-promised": "~2.0.1",
    "mocha": "~2.2.5",
    "eslint-plugin-react": "~2.3.0"
  },
  "dependencies": {
    "express": "^4.12.3",
    "forever": "^0.14.1",
    "superagent": "~1.2.0",
    "lodash": "~3.8.0",
    "rc": "~1.0.1",
    "cookie-parser": "~1.3.4",
    "body-parser": "~1.12.4",
    "aws-sdk": "~2.1.28",
    "xml2js": "~0.4.8"
  }
}
